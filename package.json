{
  "name": "@inf-monkeys/ui-themes",
  "version": "0.1.0",
  "description": "Monkeys UI Theme Components Library - OEM customization components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./registry": {
      "import": "./dist/registry/index.mjs",
      "require": "./dist/registry/index.js",
      "types": "./dist/registry/index.d.ts"
    },
    "./themes/*": {
      "import": "./dist/themes/*/index.mjs",
      "require": "./dist/themes/*/index.js",
      "types": "./dist/themes/*/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inf-monkeys/monkeys-ui-themes.git"
  },
  "keywords": [
    "monkeys",
    "ui",
    "themes",
    "oem",
    "components",
    "react"
  ],
  "license": "MIT"
}
